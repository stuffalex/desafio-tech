{
  "version": 2,
  "builds": [
    {
      "src": "./run.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/fornecedores",
      "dest": "/api/fornecedores"
    }
  ],
  {
    "src": "/fornecedores",
    "dest": "/api/fornecedores"
  }
}